{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: ["config:recommended"],
  enabledManagers: ["asdf", "gomod"],
  packageRules: [
    {
      matchManagers: ["asdf"],
      matchPackageNames: ["golang"],
      matchUpdateTypes: ["minor", "patch"],
      automerge: true,
    },
    {
      matchManagers: ["gomod"],
      matchUpdateTypes: ["minor", "patch"],
      automerge: true,
    },
  ],
}
